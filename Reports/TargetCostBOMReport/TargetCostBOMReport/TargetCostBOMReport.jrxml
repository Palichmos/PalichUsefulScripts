	<parameter name="AD_User_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT
         (SELECT u.Name FROM AD_User u WHERE u.ad_user_id = $P{AD_User_ID}) AS UserName,
         to_char(now()::timestamp without time zone, 'dd.MM.yyyy  HH24:MI') AS dateCreatedReport,
         to_char($P{DateStart1}::timestamp without time zone, 'dd.MM.yyyy  HH24.MI') AS DateStart1,
         to_char($P{DateStart2}::timestamp without time zone, 'dd.MM.yyyy  HH24.MI') AS DateStart2,
         p.name AS sel_product,
         f0.LevelNo,
         f0.TreeNo,
         lpad(f0.LevelNo::TEXT, (f0.LevelNo * 2)::integer, '_'::TEXT)
	|| CASE WHEN f0.LevelNo > 0 THEN '_' ELSE '' END
	|| concat(p2.name,'[', puom.uomsymbol,']', ' (', p2.value, ')')::CHARACTER VARYING(255) AS productbom,
         f0.Category,
         CASE WHEN f0.QtyBom1 = 0 THEN NULL ELSE f0.QtyBom1 END AS QtyBom1,
         CASE WHEN f0.IsQtyPercentage1 = 'Y' THEN '%' ELSE uom1.uomsymbol END AS bomUom1,
         CASE WHEN f0.BlUomQtyRequiered1 = 0 THEN NULL ELSE f0.BlUomQtyRequiered1 END AS BlUomQtyRequiered1,
         uom1.uomsymbol AS bomLine1_UOM,
         f0.Price1,
         CASE WHEN f0.COST1 = 0 THEN NULL ELSE round(f0.COST1, 2) END AS COST1,
         f0.description1,
         CASE WHEN f0.QtyBom2 = 0 THEN NULL ELSE f0.QtyBom2 END AS QtyBom2,
         CASE WHEN f0.IsQtyPercentage2 = 'Y' THEN '%' ELSE uom2.uomsymbol END AS bomUom2,
         CASE WHEN f0.BlUomQtyRequiered2 = 0 THEN NULL ELSE f0.BlUomQtyRequiered2 END AS BlUomQtyRequiered2,
         uom2.uomsymbol AS bomLine2_UOM,
         f0.Price2,
         CASE WHEN f0.COST2 = 0 THEN NULL ELSE round(f0.COST2, 2) END AS COST2,
         f0.description2,
         CASE WHEN f0.deltaCost = 0 THEN NULL ELSE f0.deltaCost END AS deltaCost,
         CASE WHEN f0.deltaQty = 0 THEN NULL ELSE f0.deltaQty END AS deltaQty,
         CASE WHEN f0.deltaPrice = 0 THEN NULL ELSE f0.deltaPrice END AS deltaPrice,
         CASE WHEN f0.effectQtyCost = 0 THEN NULL ELSE f0.effectQtyCost END AS effectQtyCost,
         CASE WHEN f0.effectPriceCost = 0 THEN NULL ELSE f0.effectPriceCost END AS effectPriceCost,
         CASE WHEN f0.effectQtyCostPersent = 0 THEN NULL ELSE f0.effectQtyCostPersent END AS effectQtyCostPersent,
         CASE WHEN f0.effectPriceCostPersent = 0 THEN NULL ELSE f0.effectPriceCostPersent END AS effectPriceCostPersent,
         CASE WHEN f0.checker1 = 0 THEN NULL ELSE f0.checker1 END AS checker1,
         CASE WHEN f0.checker2 = 0 THEN NULL ELSE f0.checker2 END AS checker2
    FROM target_cost_bom($P{M_Product_ID},
                          $P{PP_Product_BOM_ID},
                          $P{DateStart1}::timestamp WITHOUT time ZONE,
                          $P{DateStart2}::timestamp WITHOUT time ZONE,
                          $P{QtyRequiered},
                          'Y', -- calculateCost
                          'Y', -- showComponentNextLevel
                          'N', -- showVariants
                          'N'  -- showOnlyFirstLevel
		    ) f0
    JOIN m_product p ON f0.sel_product_ID = p.m_product_ID
    LEFT JOIN C_UOM uom1 ON uom1.C_UOM_ID = f0.BlUom1_C_UOM_ID
    LEFT JOIN C_UOM uom2 ON uom2.C_UOM_ID = f0.BlUom2_C_UOM_ID
    LEFT JOIN m_product p2 ON f0.m_productbom_ID = p2.m_product_ID
    LEFT JOIN M_Product_Category pc ON p2.M_Product_Category_ID = pc.M_Product_Category_ID
    LEFT JOIN c_uom puom ON p2.c_uom_ID = puom.c_uom_ID
   ORDER BY f0.TreeNo;]]>
	</queryString>
